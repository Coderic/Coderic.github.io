{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "coderic-org": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/index.html",
              "src/assets",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootswatch/dist/cerulean/bootstrap.min.css",
              "src/custom-theme.scss",
              "src/styles.css"
            ],
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "coderic-org:build:production"
            },
            "development": {
              "browserTarget": "coderic-org:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "coderic-org:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "browsers": "ChromeHeadless",
            "sourceMap": false,
            "codeCoverage": true,
            "watch": false,
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "version": 2,
            "browserTarget": "coderic-org:build:production"
          }
        }
      }
    },
    "suadmin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/suadmin",
      "sourceRoot": "projects/suadmin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/admin",
            "index": "projects/suadmin/src/index.html",
            "main": "projects/suadmin/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/suadmin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/suadmin/src/favicon.ico",
              "projects/suadmin/src/index.html",
              "projects/suadmin/src/assets",
              "projects/suadmin/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/suadmin/src/custom-theme.scss",
              "projects/suadmin/src/styles.css"
            ],
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "projects/suadmin/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "suadmin:build:production"
            },
            "development": {
              "browserTarget": "suadmin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "browsers": "ChromeHeadless",
            "sourceMap": false,
            "codeCoverage": true,
            "watch": false,
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/suadmin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/suadmin/src/favicon.ico",
              "projects/suadmin/src/index.html",
              "projects/suadmin/src/assets",
              "projects/suadmin/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/suadmin/src/custom-theme.scss",
              "projects/suadmin/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "panel": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/panel",
      "sourceRoot": "projects/panel/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/panel",
            "index": "projects/panel/src/index.html",
            "main": "projects/panel/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/panel/tsconfig.app.json",
            "assets": [
              "projects/panel/src/favicon.ico",
              "projects/panel/src/assets",
              {
                "glob": "es-ES.json",
                "input": "./node_modules/datatables.net-plugins/i18n/",
                "output": "./assets/"
              },
              "projects/panel/src/manifest.webmanifest"
            ],
            "styles": [
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootswatch/dist/cerulean/bootstrap.min.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/datatables.net-bs5/css/dataTables.bootstrap5.min.css",
              "node_modules/datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css",
              "node_modules/datatables.net-datetime/dist/dataTables.dateTime.min.css",
              "projects/panel/src/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.js",
              "node_modules/datatables.net/js/jquery.dataTables.min.js",
              "node_modules/datatables.net-bs5/js/dataTables.bootstrap5.min.js",
              "node_modules/pdfmake/build/pdfmake.min.js",
              "node_modules/pdfmake/build/vfs_fonts.js",
              "node_modules/jszip/dist/jszip.js",
              "node_modules/datatables.net-buttons-bs5/js/buttons.bootstrap5.min.js",
              "node_modules/datatables.net-buttons/js/buttons.html5.js",
              "node_modules/datatables.net-buttons/js/buttons.print.js",
              "node_modules/datatables.net-datetime/dist/dataTables.dateTime.min.js"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "projects/panel/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "panel:build:production"
            },
            "development": {
              "browserTarget": "panel:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "panel:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/panel/tsconfig.spec.json",
            "assets": [
              "projects/panel/src/favicon.ico",
              "projects/panel/src/assets",
              "projects/panel/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/panel/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}